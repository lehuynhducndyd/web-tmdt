    "@prisma/client": "^6.3.0",
    "axios": "^1.13.2",
    "bcrypt": "5.1.1",
    "chart.js": "^4.5.1",
    "connect-mongo": "^5.1.0",
    "dotenv": "16.4.7",
    "ejs": "3.1.10",
