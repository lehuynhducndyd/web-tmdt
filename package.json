  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "node_modules"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src/app.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lehuynhducndyd/nodejs-pro-hoidanit.git"
