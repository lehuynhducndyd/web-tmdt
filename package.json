    "@types/express": "^5.0.0",
    "@types/express-session": "1.18.1",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "@types/passport": "1.0.17",
    "@types/passport-local": "1.0.38",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.9",
